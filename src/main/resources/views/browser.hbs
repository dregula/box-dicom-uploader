<html>
  <head>
  </head>
  <body>
    <p>Box Folder Browser</p>
    <p>
      {{#if folders}}
        {{#each folders}}
          <a href="/app/browser/{{id}}">{{name}}</a>
          <br/>
        {{/each}}
      {{else}}
        No folders found
      {{/if}}
    </p>
    <form action="/app/upload/{{id}}" method="POST">
      <input type="submit" value="Upload here">
    </form>
  </body>
</html>